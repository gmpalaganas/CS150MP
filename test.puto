bilang_puno(){
    @bilang_a = 0;
    @bilang_k = 5 + 3;
    @tuldok_pi = 3.1415;

    ~
        Eto yung magic na sinasabi ko sa if-else
        di ba oh? Medyo na pa ooooooooh ako nung na realize ko
        Genius C
    ~
    kung ( bilang_a < 3 && bilang_a > 1 ){

        @salita_s = "Hello World";

        sulat(salita_s);
        #This here is a nested compound statement
        @bilang_j = 0;
        tuwing(bilang_j; bilang_j < 3; bilang_j++){
            sulat(bilang_j);
            sulat("\n");
        }

    }iba kung( bilang_a < 1)
        sulat("wala eh");
    iba{
        @salita_input = kuha();
        sulat(salita_input);
    }
    
    @bilang_i = 0;
    habang(bilang_i > 10){
        sulat(bilang_i);
        sulat("\n");
        bilang_i++;
    }
    
    @bilang_arr[3] = { 1, 2, 3 };
    tuwing(bilang_i = 0; bilang_i < 3; bilang_i++)
        sulat(bilang_arr[bilang_i]);

    bilang_i = 0;

    gawin{
        sulat(bilang_i);
        sulat("\n");
        bilang_i++;
    }habang(bilang_i > 10);

    @letra_hello = 'a';
    
}
